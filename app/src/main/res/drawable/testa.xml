
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="12dp"
        android:bottom="12dp"
        android:top="12dp"
        android:right="-2dp"
        >
        <shape>
            <solid android:color="#00000000"/>
            <stroke
                android:width="1dp"
                android:color="#63a219"
                android:dashWidth="6dp"
                android:dashGap="3dp"/>
            <corners
                android:topLeftRadius="20dp"
                android:bottomLeftRadius="20dp"/>
        </shape>
    </item>
</layer-list>
